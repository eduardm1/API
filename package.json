{
  "name": "api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node build/index.js",
    "predev": "npm run swagger",
    "prebuild": "npm run swagger",
    "build": "tsc",
    "dev": "concurrently \"nodemon\" \"nodemon -x tsoa spec\"",
    "swagger": "tsoa spec",
    "docker-build": "docker && build -t && express-ts .",
    "compose": "docker-compose up",
    "prisma-push": "npx prisma db push --preview-feature",
    "test:unit": " jest  --coverage --detectOpenHandles --runInBand"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts",
    "exec": "ts-node src/index.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.20",
    "@types/morgan": "^1.9.2",
    "@types/node": "^14.14.31",
    "@types/swagger-ui-express": "^4.1.2",
    "concurrently": "^6.0.0",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "ts-jest": "^26.5.3",
    "ts-node": "^9.1.1"
  },
  "dependencies": {
    "@prisma/client": "^2.18.0",
    "@types/supertest": "^2.0.10",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jest-rest-preset": "^0.0.4",
    "morgan": "^1.10.0",
    "pg": "^8.5.1",
    "prisma": "^2.18.0",
    "reflect-metadata": "^0.1.13",
    "supertest": "^6.1.3",
    "swagger-ui-express": "^4.1.6",
    "tsoa": "^3.5.2",
    "typeorm": "^0.2.31",
    "typescript": "^4.2.2"
  }
